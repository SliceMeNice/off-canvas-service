{
  "name": "@slicemenice/off-canvas-service",
  "version": "2.5.0",
  "description": "A lightweight service that manages off-canvas views. The top view always defines the dimensions of the body element.",
  "keywords": [
    "off-canvas",
    "modal",
    "dialog"
  ],
  "scripts": {
    "build:dist": "n-clean ./dist/ && rollup -c rollup.config.js && uglifyjs ./dist/offCanvasService.js --output ./dist/offCanvasService.min.js --compress --mangle",
    "postinstall": "npm run typings:install",
    "release-to-branch": "branch-release -s build:dist",
    "test": "echo \"Tests should be here\" && exit 0",
    "typings:install": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SliceMeNice/off-canvas-service.git"
  },
  "bugs": {
    "url": "https://github.com/SliceMeNice/off-canvas-service/issues"
  },
  "homepage": "https://github.com/SliceMeNice/off-canvas-service",
  "contributors": [
    {
      "name": "Stefan Ullinger",
      "email": "stefan@slicemenice.de"
    }
  ],
  "typings": "sources/offCanvasService.d.ts",
  "author": "SliceMeNice",
  "license": "MIT",
  "devDependencies": {
    "branch-release": "0.3.2",
    "npm-build-tools": "2.2.5",
    "rollup": "0.25.7",
    "rollup-plugin-typescript": "0.6.1",
    "typescript": "1.8.9",
    "typings": "0.7.9",
    "uglify-js": "2.6.2"
  }
}
